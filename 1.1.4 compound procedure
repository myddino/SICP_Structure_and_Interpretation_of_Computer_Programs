   (define (square x) (* x x)) 
// (define (<name> <formal parameters>) <body>)

(square 21) // 441

(square (square 3)) // 81


(+ (square x) (square y)) // x^2 + y^2


(define (sum-of-squares x y)
  (+ (square x) (square y))
 
(sum-of-squares 3 4) // 25


(define (f a)
  (sum-of-squares (+ a 1) (* a 2))
  
(f 5) // 136
 
